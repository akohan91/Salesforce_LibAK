	public MockDataCreator createMockData() {

		Map<Id, Account> accountsMap = new Map<Id, Account>((List<Account>)JSON.deserialize(this.accoountsJSON, List<Account>.class));
		List<Contact> contacts = (List<Contact>)JSON.deserialize(this.contactsJSON, List<Contact>.class);
		List<Opportunity> opportunities = (List<Opportunity>)JSON.deserialize(this.opportunitiesJSON, List<Opportunity>.class);
		List<Case> cases = (List<Case>)JSON.deserialize(this.casesJSON, List<Case>.class);

		fflib_ISObjectUnitOfWork uow = new fflib_SObjectUnitOfWork(
			new List<Schema.SObjectType>{
				Account.sObjectType,
				Contact.sObjectType,
				Opportunity.sObjectType,
				Case.sObjectType
			}
		);

		new IdsCleaner(accountsMap.values()).cleanIds();
		new IdsCleaner(contacts).cleanIds();
		new IdsCleaner(opportunities).cleanIds();
		new IdsCleaner(cases).cleanIds();

		for (Account acc : accountsMap.values()) {
			uow.registerNew(acc);
		}

		for (Contact con : contacts) {
			Account acc = accountsMap.get(con.AccountId);
			con.AccountId = acc.Id;
			uow.registerNew(con, Contact.AccountId, acc);
		}

		for (Opportunity opp : opportunities) {
			Account acc = accountsMap.get(opp.AccountId);
			opp.AccountId = acc.Id;
			uow.registerNew(opp, Opportunity.AccountId, acc);
		}

		for (Case caseItem : cases) {
			Account acc = accountsMap.get(caseItem.AccountId);
			caseItem.AccountId = acc.Id;
			uow.registerNew(caseItem, Case.AccountId, acc);
		}

		uow.commitWork();
		return this;
	}

	public class IdsCleaner {
		private List<Sobject> sobjects;

		public IdsCleaner(List<Sobject> sobjects) {
			this.sobjects = sobjects;
		}

		public void cleanIds() {
			for (Sobject obj : this.sobjects) {
				obj.Id = null;
			}
		}
	}
